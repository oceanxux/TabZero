{
  "manifest_version": 3,
  "name": "TabZero",
  "version": "1.0.0",
  "description": "极简风的新标签页 - 书签管理、快捷访问、多搜索引擎",
  "permissions": [
    "storage",
    "bookmarks",
    "topSites"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}